<Window x:Class="PruebaKairos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PruebaKairos"
        mc:Ignorable="d"
        Title="Pruebas con Kairos" Height="350" Width="525">
    <Grid>
        <Button x:Name="seekImage" Content="Seleccionar imagen" HorizontalAlignment="Left" Margin="67,88,0,0" VerticalAlignment="Top" Width="auto" Click="seekImage_Click"/>
        <TextBox x:Name="url" HorizontalAlignment="Left" Height="23" Margin="67,43,0,0" TextWrapping="Wrap" Text="Ruta de la imagen" VerticalAlignment="Top" Width="372"/>
        <TextBox x:Name="answer" HorizontalAlignment="Left" Height="87" Margin="67,134,0,0" TextWrapping="Wrap" Text="Aquí el resultado" VerticalAlignment="Top" Width="362"/>

    </Grid>
</Window>
